@{

}

<!DOCTYPE html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />

  <title>@Page.Title</title>
  <meta name="description" content="@Page.Description" />
  <meta name="author" content="@Page.Author" />

  <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />

  <link href="~/dist/css/bootstrap.css" rel="stylesheet" />
  @RenderSection("styles", required: false)
</head>
<body>
  @RenderBody()

  @* JavaScript at the bottom for faster page loading *@
  @* Grab jQuery, with a protocol relative URL; fall back to local if offline *@
  @if (Request.Browser.Browser == "IE" && Request.Browser.Version.AsDecimal() < 9)
  {
    <text>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    </text>
  }
  else
  {
    <text>
      <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.1.4.min.js"></script>
      <script>window.jQuery || document.write('<script src="@Href("~/dist/js/jquery-2.1.4.min.js")"><\/script>')</script>
    </text>
  }

  <script src="~/dist/js/bootstrap.js"></script>
  @RenderSection("scripts", required: false)
</body>
</html>
